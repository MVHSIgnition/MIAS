{
   "background": {
      "persistent": false,
      "scripts": [ "browser.js", "getTabSource.js", "eventPage.js" ]
   },
   "description": "This extension shows an icon denoting the political bias for the current page.",
   "key": "",
   "manifest_version": 2,
   "name": "MIAS",
   "page_action": {
      "default_popup": "popup.html",
      "default_icon": "mainIcon.png",
      "default_title": "MIAS"
   },
   "content_scripts": [{
     "css": ["styles.css"],
     "js": ["content.js", "eventPage.js", "popup.js", "browser.js", "getTabSource.js"],
     "matches": ["<all_urls>"]
   }],
   "permissions": [ "alarms", "storage", "tabs" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.1"
}
